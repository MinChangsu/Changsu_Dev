package day17;
import java.util.*;
public class qz02 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		Random rn=new Random();
		boolean jd=false;
		int n1;
		System.out.println("숫자입력");
		int num=sc.nextInt();
		for(int i=0;i<10;i++) {
			n1=rn.nextInt(1,11);
			if(n1==num) {
				System.out.println("정답");
				jd=true;
				break;}
}
		if(jd==false) {
			System.out.println("기회끝");
		}
		
	}

}
