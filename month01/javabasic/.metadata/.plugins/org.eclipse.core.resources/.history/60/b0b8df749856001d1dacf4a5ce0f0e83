package day15;



class Box{
	private String conts;
	Box(String cont){
		this.conts=cont;
		}
	public String toString(){
		return conts;
	}
	
}
public class tostring {

	public static void main(String[] args) {
		StringBuilder stb =new StringBuilder("12");
		stb.append(34);
		System.out.println(stb.toString());
		System.out.println(stb);
		
		

	}

}
