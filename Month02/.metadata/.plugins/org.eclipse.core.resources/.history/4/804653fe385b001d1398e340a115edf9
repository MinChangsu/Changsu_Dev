package day18;
enum Animal{
	Dog,Cat,Hamster,Turtle
}

class Phone{
	enum Company{
		Apple,Samsung
	}
	Company c;
	Phone(String c_name){
		if(c_name.equals("애플")) {
		c=Company.Apple;}
		else if(c_name.equals("삼성")) {
			c=Company.Samsung;
		}
	}
}

public class Enum01 {

	public static void main(String[] args) {
		Animal dog =Animal.Dog;
		switch(dog) {
		case Cat:
			System.out.println("야옹");
			break;
		case Dog:
			System.out.println("멍 멍");
		break;}
		
		Phone p1 =new Phone("삼성");
		System.out.println(p1.c);
	}

}
