package day18;
import java.util.*;
public class Test {

	public static void main(String[] args) {
		List<String> sli=new ArrayList();
	sli.add("Banana");	
	sli.add("Carrot");	
	sli.add("Apple");
	Collections.sort(sli);
	
	int inx =Collections.binarySearch(sli, "Carrot");
	System.out.println(inx);
	}

}
