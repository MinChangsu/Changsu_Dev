package day19;
interface Calculator<T>{
	T cal(T a,T b);
}
public class Qz01 {
	public static <T> void calandshow(Calculator<T> op,T n1, T n2) {
		T r=op.cal(n1, n2);
		System.out.println(r);
	}

	public static void main(String[] args) {
		Calculator<Integer> x=(a,b)->a+b;
		Qz01.calandshow(x, 5, 8);
	}

}
